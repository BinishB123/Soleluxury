                            </select>
                        </div>
                    </div> -->
                </header> <!-- card-header end// -->
                <div class="card-body">
                    <div class="card-body">
                        <table class="table" id="customerTable">
                            <thead style=" background-color: black;">
                                <tr style="color: white;">
                                    <th>OrderId</th>
                                    <th>Name</th>
                                    <th>Ordered On</th>
                                    <th>TotalAmount</th>
                                    <th>payment-method</th>
                                    <th>View Detail</th>
                                </tr>
                                </tr>
                            </thead>
                            <tbody>
                                <% for( let i = 0; i < order.length; i++ ) { %>
                                
                               
                                <tr>
                                    <td>
                                        <div class="itemside">
                                            <div class="left">
                                                <span class=""><%= order[i].orderId %>
                                            </span>
                                                
                                            </div>
                                            <!-- <div class="info">
                                                <h6 class="mb-0"> </h6>
                                            </div> -->
                                        </div>
                                    </td>
                                    <td>
                                        <span class=""><%= order[i].user.name %>
                                            </span>
                                    </td>
                                    <td>
                                        <span class=""><%= order[i].orderedOn.toLocaleDateString()%>
                                            </span>
                                    </td>
                                    <td>
                                        <span class="">₹ <%= order[i].totalAmount %>
                                            </span>
                                    </td>
                                    <td>
                                        <span class=""><%= order[i].paymentMethod %>
                                            </span>
                                    </td>
                                   
                                    <td>
                                        <a class="btn btn-sm"  href="/admin/adminOrderDetail?id=<%=order[i]._id %>" style="background-color: black;color:white;"> view</a>
                                    </td>
                                    
                                    
                                    
                                     
                                  
                                </tr>
                                <% } %>
                            </tbody>
                        </table>
                        <% if (totalPages){ %>
                            <nav aria-label="Page navigation example" >
                                <ul class="pagination justify-content-center">
                                <% if (currentPage > 1) { %>
                                  <li class="page-item ">
                                    <a class="page-link" href="/admin/orders?page=<%=currentPage-1%>" >Previous</a>
                                  </li>
                                  <% } %>
                                 
                                  <li class="page-item"><a class="btn btn-sm active" ><%=currentPage%></a></li>
                                
                                  <% if (currentPage < totalPages) { %>
                                  <li class="page-item">
                                    <a class="page-link" href="/admin/orders?page=<%=currentPage+1%>">Next</a>
                                  </li>
                                  <% } %>
                                </ul>
                              </nav>
                              <% } %>
                        </div>
                    </div>
                    
                            </div>
                            
                        </div>
                        
                    </div>
                    
                   
                
                    <!-- itemlist .// -->
                
                </div> <!-- card-body end// -->
                
                
                
