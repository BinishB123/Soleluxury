<%- include("../partials/adminHeader") %>
        <section class="content-main" style="background-color: white;">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Orders List</h2>
                   
                </div>
                <% if (locals.message) { %>
                    <span class="text-center w-50 mx-auto" style=" display: block;  color: rgb(23, 200, 14);" id="insertmessage"><%=message %></span>
                 
                <% } %>
                
                
               
                <div>
                    <!-- <a href="#" class="btn btn-light rounded font-md">Export</a>
                    <a href="#" class="btn btn-light rounded  font-md">Import</a> -->
                    <!-- <a href="/admin/addproducts" class="btn  btn-sm rounded" style="background-color: rgb(15, 148, 224); color: white;">Add Product</a> -->
                </div>
                
            </div>
            <div class="card mb-4">
                <header class="card-header">
                    <!-- <div class="row align-items-center">
                        <div class="col col-check flex-grow-0">
                            <div class="form-check ms-2">
                                <input class="form-check-input" type="checkbox" value="">
                            </div>
                        </div>
                        <div class="col-md-3 col-12 me-auto mb-md-0 mb-3">
                            <select class="form-select">
                                <option selected>All category</option>
                                <option>Electronics</option>
