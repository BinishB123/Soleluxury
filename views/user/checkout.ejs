    let couponId
    function useCoupon(couponid) {

        const couponOffer = document.getElementById("couponOffer")
        const coupontd = document.getElementById("coupontd")
        // console.log(couponOffer)
       couponId = couponid;
    const subtotal = document.getElementById("secondsubtotal").getAttribute("data-value");
    $.ajax({
        url: `/use-coupon?id=${couponid}`,
        method: 'POST',
        contentType: 'application/json',
        data: JSON.stringify({ subtotal: subtotal }),
        success: function(response) {
            if (response.success) {
                const couponusing = document.getElementById("couponusing")
                couponusing.style.display="none"
                hideCouponModal()
                const newTotalPrice = response.newPrice;
                // console.log(newTotalPrice);
                const fistSubTotal = document.getElementById("firstSubTotal");
                const secondsubtotal = document.getElementById("secondsubtotal");
                fistSubTotal.textContent = '₹' + newTotalPrice;
                secondsubtotal.textContent = '₹' + newTotalPrice ;
                Toastify({
                    text: "Coupon Offer Added",
                    backgroundColor: "green",
                    duration: 6000,
                    position: "center"
                }).showToast();
                
                const originalPrice = firstSubTotal; // Original price
                 const discountPercentage = response.coupon.discount; // Discount percentage from the coupon

// Calculate the discount amount
                 const discountAmount = (originalPrice * discountPercentage) / 100;

// Calculate the amount to be paid after applying the discount
                   const amountToBePaid = originalPrice - discountAmount;

                couponOffer.innerHTML =`Actual price is  ${firstSubTotal} ,Coupon Applied ${response.coupon.discount}% off   <i class="bi bi-x-circle"  style="color: black;"  onclick='removeCoupon()' id="removeIcon"></i>  `
                couponOffer.style.color ="black"

                
            } else {
                Toastify({
                    text: "Cannot apply coupon",
                    backgroundColor: "red",
                    duration: 6000,
                    position: "center"
                }).showToast();
            }
        },
        error: function(error) {
            window.location.href = '/login';
        }
    });
}

function removeCoupon() {
    let confirmationMessage = "Are you sure you want to remove this coupon?";
    
    swal({
        text: confirmationMessage,
        buttons: ["Cancel", "Yes"],
    }).then((result) => {
        if (result) {
            window.location.reload();
        }
    });
}







        


