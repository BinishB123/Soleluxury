  function cancelOrder(orderid, productDocid, quantity, size, productid) {
            
    swal({
        title: "Cancel Order",
        text: "Are you sure you want to cancel this order?",
        icon: "warning",
        buttons: ["No", "Yes"],
        dangerMode: true,
    })
    .then((willCancel) => {
        if (willCancel) {
            // Send AJAX request to cancel the order
            $.ajax({
                url: '/cancelOrder', 
                method: 'PATCH', 
                data: { orderid: orderid, productDocid: productDocid, quantity: quantity, size: size,productid:productid }, 
                success: function(response) {
                    // console.log(response)
                    if (response.success === true) {
                        window.location.reload();
                        swal("Success", "Order canceled!", "success");
                    } else {
                        swal("Error", "Failed to cancel the order. Please try again later.", "error");
                    }
                },
                error: function(xhr, status, error) {
                    swal("Error", "Failed to cancel the order. Please try again later.", "error");
                    console.error('Error canceling order:', error);
                }
            });
        } else {
            swal("not Cancelled", "Your order is safe :)", "info");
        }
    });
}
