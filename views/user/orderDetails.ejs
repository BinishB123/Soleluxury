<table class="table shopping-summery text-center clean">
    <thead>
        <tr class="main-heading">
            <th scope="col">Product Image</th>
            <th scope="col">Product Name</th>
            <th scope="col">Size</th>
            <th scope="col">Price</th>
            <th scope="col">Quantity</th>
            <th scope="col">Status</th>
            <th scope="col">Action</th>
        </tr>
    </thead>
    <tbody>
        <% for(let i = 0; i < order.length; i++) { %>
        <tr id="<%= order[i]._id %>">
            <td class="image product-thumbnail">
                <img src="/public/photos/productImages/<%= order[i].product.productImage[0] %>" alt="/productview/<%= order[i].product._id %>">
            </td>
            <td class="product-des product-name">
                <h5 class="product-name"><a href="/productview/<%= order[i].product._id %>"><%= order[i].product.productName %></a></h5>
            </td>
            <td class="price" data-title="Size"><span><%= order[i].products.size %></span></td>
            <td class="price" data-title="Price">₹<span><%= order[i].products.productPrice %></span></td>
            <td class="text-center" data-title="Quantity">
                <div class="detail-qty border radius m-auto">
                    <span><%= order[i].products.quantity %></span>
                </div>
            </td>
            <td class="action" data-title="Status">
                <% if(order[i].products.status === "cancelled") { %>
                    <span class="badge" style="background-color: red;"><%= order[i].products.status %></span>
                <% } else if(order[i].products.status === "pending") { %>
                    <span class="badge" style="background-color: rgb(0, 149, 255);"><%= order[i].products.status %></span>
                <% } else if(order[i].products.status === "Processing") { %>
                    <span class="badge" style="background-color: rgb(255, 179, 0);"><%= order[i].products.status %></span>
                <% } else if(order[i].products.status === "Confirmed") { %>
                    <span class="badge" style="background-color: rgb(255, 208, 0);"><%= order[i].products.status %></span>
                <% } else if(order[i].products.status === "Shipped") { %>
                    <span class="badge" style="background-color: rgb(179, 212, 15);"><%= order[i].products.status %></span>
                <% } else if(order[i].products.status === "Delivered") { %>
                    <span class="badge" style="background-color: rgb(8, 230, 45);"><%= order[i].products.status %></span>
                <% } else if(order[i].products.status === "return Pending") { %>
                    <span class="badge" style="background-color: rgb(220, 152, 17);"><%= order[i].products.status %></span>
                <% } else if(order[i].products.status === "OutForDelivery") { %>
                    <span class="badge" style="background-color: rgb(31, 229, 17);"><%= order[i].products.status %></span>
                <% } else if(order[i].products.status === "Returned") { %>
                    <span class="badge" style="background-color: rgb(18, 52, 15);"><%= order[i].products.status %></span>
                <% } else if(order[i].products.status === "payment pending") { %>
                    <span class="badge" style="background-color: rgb(159, 33, 92);"><%= order[i].products.status %></span>
                <% } %>
            </td>
            
            <td class="action" data-title="Action">
                <% if(order[i].products.status === "cancelled") { %>
                    <span style="background-color: rgb(255, 255, 255); color: rgb(235, 19, 19);">Cancelled</span>
                <% } else if (order[i].products.status === "Delivered") { %>
                    <a href="#" id="" class="btn btn-sm" style="background-color: rgb(0, 132, 255); color: white;" onclick="returnOrder('<%= order[i]._id %>', '<%= order[i].products._id %>')">Return</a>
                    <a class="btn btn-sm" onclick="dldInvoice('<%= order[i]._id %>', '<%= order[i].products._id %>')" style="background-color: black;">Download invoice</a>
                <% } else if(order[i].products.status === "return Pending") { %>
                    <span class="badge" style="color: rgb(133, 133, 133);">Return pending</span>
                <% } else if(order[i].products.status === "Returned") { %>
                    <span class="badge" style="color: rgb(13, 42, 184);">Returned</span>
                
                <%}else { %>
                    <a href="#" id="" class="btn btn-sm" style="background-color: red; color: white;" onclick="cancelOrder('<%= order[i]._id %>', '<%= order[i].products._id %>','<%=order[i].products.quantity%>','<%=order[i].products.size%>','<%=order[i].product._id%>')">Cancel</a>
                <% } %>
            </td>
        </tr>
        <% } %>
    </tbody>
</table>

<div class="text-end">
