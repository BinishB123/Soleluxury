  function dldInvoice(orderid, productDocid) {
  $.ajax({
    url: '/invoicedownload',
    method: 'POST',
    data: { orderid: orderid, productDocid: productDocid },
    success: function(response) {
      if (response.data) {
        const details = response.data;
        const currentDate = new Date(); // Create a new Date object representing the current date and time
        const formattedDate = currentDate.toLocaleDateString();
        // console.log(formattedDate)
        // Prepare data for creating invoice
        var data = {
          apiKey: "free",
          mode: "development",
          images: {
            logo: "https://public.budgetinvoice.com/img/logo_en_original.png",
          },
          sender: {
            company:"Solelucury",
            address: " kundanoor maradu",
            zip: "AdC",
            city: "Kochi",
            country: "India"
          },
          client: {
            company: details.address.name,
            address: details.address.house,
            city: details.address.city,
            country: details.address.country
          },
          information: {
            number: "2021.0001",
            date: formattedDate,
            
          },
          products: [
            {
              quantity: details.products.quantity,
              description: details.prod.productName,
              price: details.products.productPrice
            }
          ],
          bottomNotice: "",
          settings: {
            currency: "INR"
          }
        };
        // Create the invoice
        easyinvoice.createInvoice(data, function (result) {
          
          easyinvoice.download('Soleluxury-invoice.pdf', result.pdf);
        });
      } else {
        swal("Error", "Failed to fetch individual order data.", "error");
      }
    },
    error: function(xhr, status, error) {
      swal("Error", "Failed to download invoice. Please try again later.", "error");
      console.error('Error downloading invoice:', error);
    }
  });
}
        function goBack() {
            window.history.back();
